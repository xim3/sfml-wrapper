[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/xim3
[access "refs/for/refs/*"]
	read = group user/xim3
	push = group user/xim3
	label-Code-Review = -2..+2 group user/xim3
	label-Verified = -1..+1 group user/xim3
	rebase = group user/xim3
	removeReviewer = group user/xim3
	submit = group user/xim3
[access "refs/heads/*"]
	read = group user/xim3
	create = group user/xim3
	push = +force group user/xim3
	pushMerge = group user/xim3
[access "refs/tags/*"]
	push = +force group user/xim3
	pushTag = group user/xim3
	pushSignedTag = group user/xim3
